<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">

    </head>
    <body>
        <%- include('partials/header') %>
        <table class="table table-bordered table-hover" style="margin-top: 50px;">
            <thead class="table-light">
              <tr>
                <th scope="col">Name</th>
                <th scope="col">Author</th>
                <th scope="col">Title</th>
                <th scope="col">Describtion</th>
                <th scope="col">Published on</th>
              </tr>
            </thead>
            <% for(let i=0;i<result[0].articles.length;i++){%>
               
            <tbody>
              <tr>
                <td><%= result[0].articles[i].source.name %></td>
                <td><%= result[0].articles[i].author %></td>
                <td><%= result[0].articles[i].title %></td>
                <td><%= result[0].articles[i].description %></td>
                <td><%= result[0].articles[i].publishedAt %></td>
              </tr>
            </tbody>
            <%}%>
          </table>

          <%- include('partials/footer') %>
    </body>
</html>